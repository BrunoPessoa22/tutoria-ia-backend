{
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "env": {
    "ANTHROPIC_API_KEY": "@anthropic_api_key",
    "ELEVENLABS_API_KEY": "@elevenlabs_api_key",
    "HEYGEN_API_KEY": "@heygen_api_key",
    "DATABASE_URL": "@database_url",
    "JWT_SECRET": "@jwt_secret",
    "REDIS_URL": "@redis_url"
  }
}